{
  "name": "template-node-api-app",
  "version": "1.0.0",
  "description": "A template for building Node.js APIs using Express",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc -p tsconfig.build.json",
    "start": "node dist/app.js",
    "dev": "nodemon",
    "serve": "npx tsc -w & nodemon dis/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint ./src/*",
    "format": "prettier --write src/**/*.{js,ts}"
  },
  "keywords": [
    "template",
    "node",
    "express",
    "typescript"
  ],
  "author": "Yuvraj Gill",
  "license": "MIT",
  "dependencies": {
    "apicache": "^1.6.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
